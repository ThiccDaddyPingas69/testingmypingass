    function follow_user (username) {
        var request = new XMLHttpRequest();
        request.open('GET', '/community/quick-follow/' + username + '/');
        request.send();
        var elements = document.getElementsByClassName("btn__follow__" + username);
        while (elements.length > 0) {
            elements[0].remove();
        }
    }
